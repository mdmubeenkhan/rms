create table users(
   id INT NOT NULL AUTO_INCREMENT,
   username VARCHAR(20) NOT NULL,
   password VARCHAR(20) NOT NULL,
   usertype VARCHAR(20) NOT NULL,
   PRIMARY KEY ( id )
);

query = INSERT INTO users(username, password, usertype) VALUES("admin","admin","admin");
INSERT INTO users(username, password, usertype) VALUES("user","user","user");
Record inserted

create table requirements(
   ID INT NOT NULL,
   VERSION INT NOT NULL,
   SUMMARY VARCHAR(100) NOT NULL,
   NAME VARCHAR(20) NOT NULL,
   PROJECT VARCHAR(20) NOT NULL,
   TYPE enum('Functional', 'Non-Functional', 'Domain', 'Interface') NOT NULL,
   SEVERITY enum('Low', 'Medium', 'High') NOT NULL,
   PRIORITY enum('Low', 'Medium', 'High') NOT NULL,
   DESCRIPTION VARCHAR(1000) NOT NULL,
   CREATED_BY VARCHAR(20) NOT NULL,
   CREATED_AT datetime NOT NULL,
   STATUS char(1)
);

create table save(   
   USERNAME VARCHAR(20) NOT NULL,
   SUMMARY VARCHAR(100) NOT NULL,
   NAME VARCHAR(20) NOT NULL,
   PROJECT VARCHAR(20) NOT NULL,
   TYPE enum('Functional', 'Non-Functional', 'Domain', 'Interface') NOT NULL,
   SEVERITY enum('Low', 'Medium', 'High') NOT NULL,
   PRIORITY enum('Low', 'Medium', 'High') NOT NULL,
   DESCRIPTION VARCHAR(1000) NOT NULL   
);


